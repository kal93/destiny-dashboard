<?xml version='1.0' encoding='utf-8'?>
<widget id="net.destinydashboard" version="1.2.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name> Destiny Dashboard </name>
    <description> Manage your Destiny 2 characters with Destiny Dashboard. </description>
    <author email="destinydashboard@gmail.com" href="https://www.destinydashboard.com"> Destiny Dashboard </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="HeaderColor" value="#303F9F" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon.png" />
    </platform>
    <preference name="EnableViewportScale" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="228" src="res/icon-228.png" width="228" />
        <icon height="180" src="res/icon-180.png" width="180" />
        <icon height="167" src="res/icon-167.png" width="167" />
        <icon height="152" src="res/icon-152.png" width="152" />
        <icon height="144" src="res/icon-144.png" width="144" />
        <icon height="120" src="res/icon-120.png" width="120" />
        <icon height="114" src="res/icon-114.png" width="114" />
        <icon height="100" src="res/icon-100.png" width="100" />
        <icon height="87" src="res/icon-87.png" width="87" />
        <icon height="80" src="res/icon-80.png" width="80" />
        <icon height="76" src="res/icon-76.png" width="76" />
        <icon height="72" src="res/icon-72.png" width="72" />
        <icon height="60" src="res/icon-60.png" width="60" />
        <icon height="58" src="res/icon-58.png" width="58" />
        <icon height="57" src="res/icon-57.png" width="57" />
        <icon height="50" src="res/icon-50.png" width="50" />
        <icon height="40" src="res/icon-40.png" width="40" />
        <icon height="29" src="res/icon-29.png" width="29" />
        <icon height="20" src="res/icon-20.png" width="20" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-advanced-http" spec="^1.5.9" />
    <plugin name="cordova-plugin-headercolor" spec="^1.0.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
